{
  "name": "delegation-server",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/delegation-server/src",
  "tags": ["type:typescript", "scope:backend"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "apps/delegation-server"
      },
      "configurations": {
        "production": {
          "command": "npm run start"
        },
        "mock": {
          "command": "npm run start:mock"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "npm run build",
        "cwd": "apps/delegation-server"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/coverage"],
      "options": {
        "command": "npm run test",
        "cwd": "apps/delegation-server"
      },
      "configurations": {
        "watch": {
          "command": "npm run test:watch"
        },
        "coverage": {
          "command": "npm run test:coverage"
        }
      }
    },
    "test:integration": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run test:integration",
        "cwd": "apps/delegation-server"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "apps/delegation-server"
      }
    },
    "install": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm install",
        "cwd": "apps/delegation-server"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf dist",
          "rm -rf node_modules"
        ],
        "cwd": "apps/delegation-server"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run proto:build",
        "cwd": "apps/delegation-server"
      }
    }
  }
}